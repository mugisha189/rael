datasource db {
    provider     = "mongodb"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

model ProfileChangeRequest {
    id String @id @default(uuid()) @map("_id")
    userId String 
    isApproved Boolean @default(false)
}
